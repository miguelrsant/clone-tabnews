    "service:down": "docker compose -f infra/compose.yaml up down",
    "service:stop": "docker compose -f infra/compose.yaml stop",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test:jest": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "",
